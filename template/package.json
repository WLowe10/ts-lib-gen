{
	"name": "",
	"version": "",
	"description": "",
	"license": "MIT",
	"author": "",
	"keywords": [],
	"repository": {
		"type": "git",
		"url": ""
	},
	"type": "module",
	"sideEffects": false,
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"files": [
		"dist",
		"package.json",
		"README.md"
	],
	"scripts": {
		"build": "tsup src/index.ts --dts --format esm,cjs",
		"format": "prettier --write .",
		"publish-package": "pnpm build && pnpm publish",
		"clean": "pnpm clean:dist && pnpm clean:modules",
		"clean:dist": "rm -rm dist",
		"clean:modules": "rm -rm node_modules",
		"type-check": "tsc --noEmit"
	},
	"devDependencies": {
		"tsup": "^8.1.0",
		"tsx": "^4.16.2",
		"prettier": "^3.3.2",
		"typescript": "^5.5.3"
	}
}
