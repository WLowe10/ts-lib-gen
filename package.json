{
	"name": "create-ts-lib-gen",
	"version": "1.1.1",
	"description": "Quickly generate a TS library",
	"license": "MIT",
	"type": "module",
	"keywords": [
		"ts-lib-gen",
		"typescript",
		"template"
	],
	"author": {
		"name": "Wes Lowe",
		"email": "wesley.jlowe10@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/WLowe10/ts-lib-gen"
	},
	"scripts": {
		"build": "tsup src/cli.ts --dts --format esm",
		"format": "prettier --write .",
		"publish-package": "pnpm build && pnpm publish",
		"clean": "pnpm clean:dist && pnpm clean:modules && pnpm clean:dest",
		"clean:dist": "rm -rf dist",
		"clean:modules": "rm -rf node_modules",
		"clean:dest": "rm -rf dest",
		"type-check": "tsc --noEmit",
		"dev": "NODE_ENV=development tsx ./src/cli.ts"
	},
	"bin": "./dist/cli.js",
	"files": [
		"dist",
		"package.json",
		"template",
		"README.md"
	],
	"dependencies": {
		"@clack/prompts": "^0.7.0",
		"chalk": "^5.3.0",
		"execa": "^9.3.0",
		"poopgen": "^0.2.1"
	},
	"devDependencies": {
		"@types/node": "^20.14.13",
		"prettier": "^3.3.3",
		"tsup": "^8.2.3",
		"tsx": "^4.16.2",
		"typescript": "^5.5.4"
	}
}
